{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/favicon.ico",
      "dest": "public/images/anita2rounded.png"
    },
    {
      "src": "/webhook/retell",
      "dest": "main.py",
      "methods": ["POST", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "https://api.retellai.com",
        "Access-Control-Allow-Methods": "POST, OPTIONS",
        "Access-Control-Allow-Headers": "x-retell-auth, x-retell-signature, x-vercel-protection-bypass, content-type",
        "x-vercel-protection-bypass": "${VERCEL_PROTECTION_BYPASS}"
      },
      "continue": true,
      "override": true
    },
    {
      "src": "/api/webhook",
      "dest": "main.py",
      "methods": ["POST", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "https://api.retellai.com",
        "Access-Control-Allow-Methods": "POST, OPTIONS",
        "Access-Control-Allow-Headers": "x-retell-auth, x-retell-signature, x-vercel-protection-bypass, content-type",
        "x-vercel-protection-bypass": "${VERCEL_PROTECTION_BYPASS}"
      },
      "continue": true,
      "override": true
    },
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1"
  },
  "crons": [
    {
      "path": "/api/cleanup",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/process-transcripts",
      "schedule": "* * * * *"
    }
  ]
}
