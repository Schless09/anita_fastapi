{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "maxDuration": 100,
        "maxLambdaSize": "15mb",
        "runtime": "python3.12",
        "fluidCompute": true
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "buildCommand": "pip install --upgrade pip && pip install --no-cache-dir -r requirements.txt"
}
