{
  "version": 2,
  "builds": [
    {
      "src": "app/main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9",
        "requirementsPath": "./requirements.slim.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/favicon.ico",
      "dest": "app/static/favicon.ico"
    },
    {
      "src": "/webhook/retell",
      "dest": "app/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "app/main.py"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1"
  }
}
